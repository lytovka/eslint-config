{
  "name": "@lytovka/eslint-config",
  "version": "0.0.1",
  "description": "ESLint rules configuration according to Ivan Lytovka's preference.",
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "lytovka"
  ],
  "homepage": "https://github.com/lytovka/eslint-config-lytovka",
  "bugs": {
    "url": "https://github.com/lytovka/eslint-config-lytovka/issues",
    "email": "ivanlytovka@gmail.com"
  },
  "author": "Ivan Lytovka <ivanlytovka@gmail.com> (https://github.com/lytovka)",
  "main": "index.js",
  "license": "MIT",
  "private": false,
  "eslintConfig": {
    "extends": "./index.js"
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0",
    "npm": ">=6",
    "yarn": ">=1"
  },
  "engineStrict": true,
  "scripts": {
    "lint": "eslint --config index.js .",
    "find-new-rules:main": "eslint-find-rules --unused ./index.js"
  },
  "dependencies": {
    "eslint-config-prettier": "^8.5.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.10",
    "eslint": "^8.28.0",
    "eslint-find-rules": "^4.1.0",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "typescript": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
